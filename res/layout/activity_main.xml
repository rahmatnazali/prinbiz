                <Button android:id="@id/m_LifePhotoButton" android:text="@string/LIFETEXT" android:drawableTop="@drawable/button_living_pics" android:drawablePadding="3dp" style="@style/btnMainStyle" />
            </TableRow>
            <TableRow android:layout_width="-2" android:layout_height="-2">
                <Button android:id="@id/m_IDpicsButton" android:text="@string/IDTXT" android:drawableTop="@drawable/button_id_photo" android:drawablePadding="3dp" style="@style/btnMainStyle" />
                <Button android:id="@id/m_SnappicsButton" android:text="@string/SNAPTXT" android:drawableTop="@drawable/button_instant_print" android:drawablePadding="3dp" style="@style/btnMainStyle" />
            </TableRow>
        </TableLayout>
        <LinearLayout android:gravity="800005" android:orientation="0" android:id="@id/m_InfoButtomLayout" android:layout_width="86dp" android:layout_height="-2" android:layout_marginRight="15dp" android:layout_marginBottom="10dp" android:layout_alignParentRight="true" android:layout_alignParentBottom="true">
            <ImageButton android:id="@id/m_InfoButton" android:background="0x106000d" android:layout_width="-1" android:layout_height="-2" android:src="@drawable/button_info" android:layout_weight="1.0" android:contentDescription="@string/desc" />
            <ImageButton android:id="@id/m_SettingButton" android:background="0x106000d" android:layout_width="-1" android:layout_height="-2" android:src="@drawable/button_setting" android:layout_weight="1.0" android:contentDescription="@string/desc" />
