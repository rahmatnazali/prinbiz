            <TableRow android:gravity="10" android:orientation="0" android:paddingBottom="5dp" android:layout_width="-1" android:layout_height="-2">
                <ImageButton android:id="@id/m_MDNSHintImageButton" android:background="0x106000d" android:layout_width="25dp" android:layout_height="25dp" android:layout_marginRight="10dp" android:src="@drawable/star2" android:scaleType="1" android:contentDescription="@string/DESCRIPTION" />
